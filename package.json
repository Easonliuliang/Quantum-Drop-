    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@types/qrcode": "^1.5.6",
    "face-api.js": "^0.22.2",
    "html5-qrcode": "^2.3.8",
    "postprocessing": "^6.38.2",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.182.0"
