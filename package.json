    "tauri:dev": "tauri dev",
    "tauri:build": "bash scripts/tauri_build_stable.sh"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@noble/ed25519": "^2.3.0",
