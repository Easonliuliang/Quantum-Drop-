{
  "name": "courier-agent",
  "version": "0.2.15",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:ui": "vite",
    "build": "npm run build:ui",
    "build:ui": "vite build",
    "preview": "vite preview",
